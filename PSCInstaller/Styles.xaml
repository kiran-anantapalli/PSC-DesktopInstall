<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Values (Dynamic Resources)-->
    <Thickness x:Key="CommonMargin">10</Thickness>
    <sys:Double x:Key="TitleFontSize">30</sys:Double>
    <sys:Double x:Key="HeaderFontSize">18</sys:Double>
    <sys:Double x:Key="Header2FontSize">16</sys:Double>
    <sys:Double x:Key="SubjectFontSize">14</sys:Double>

    <!-- Fonts -->
    <FontFamily x:Key="CommonFontFamily">Franklin Gothic Medium</FontFamily>
    
    <!-- Brushes -->
    <SolidColorBrush x:Key="PearsonBackground" Color="#FFEC8927"></SolidColorBrush>
    <SolidColorBrush x:Key="ContrastBackground" Color="#FFEC8927"></SolidColorBrush>
    <SolidColorBrush x:Key="ContrastForeground" Color="White"></SolidColorBrush>
    <SolidColorBrush x:Key="DefaultBackgroundColorBrush" Color="#FF86B9C5" />
    <SolidColorBrush x:Key="ScreenBackground" Color="#334993" />
    <SolidColorBrush x:Key="BlueBackground" Color="#8CBFCB" />
    <SolidColorBrush x:Key="PageBackground" Color="#FFE4E4E4" />
    <SolidColorBrush x:Key="TextForeground" Color="Black" />
    <SolidColorBrush x:Key="NavigationButtonBackground" Color="#FF838383" />
    <SolidColorBrush x:Key="NavigationButtonForeground" Color="White" />
    <SolidColorBrush x:Key="NavigationButtonDisabledBackground" Color="#FFF4F4F4" />
    <SolidColorBrush x:Key="HighlightNavButtonBackground" Color="#FFC4E5F6" />

    

    <LinearGradientBrush x:Key="EmbossBrush">
        <LinearGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="White" Offset="0"/>
                <GradientStop Color="Gray" Offset="1"/>
            </GradientStopCollection>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VerticalTransparentGradient" StartPoint=".5, 0" EndPoint=".5, 1">
        <GradientStopCollection>
            <GradientStop Offset="0" Color="Transparent"/>
            <GradientStop Offset="1" Color="White"/>
        </GradientStopCollection>
    </LinearGradientBrush>

    <Style TargetType="{x:Type Border}" x:Key="PageBaseStyle">
        <Setter Property="Background" Value="{StaticResource PageBackground}"/>
        <Setter Property="Padding" Value="{StaticResource CommonMargin}"/>
    </Style>
    
    
    <!-- Images -->
    <Style x:Key="PearsonControlControlStyle" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Height="50" Width="50">
                        <Rectangle Fill="{StaticResource ScreenBackground}"></Rectangle>
                        <Image HorizontalAlignment="Center" Stretch="UniformToFill" Source="/PSCInstaller;component/Assets/Images/Logo.scale-100.png"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Text -->
    <Style TargetType="{x:Type TextBlock}" x:Key="BaseStyle">
        <Setter Property="FontFamily" Value="Franklin Gothic Medium"/>
    </Style>
    
    <Style TargetType="{x:Type TextBlock}" x:Key="Title" BasedOn="{StaticResource BaseStyle}">
        <Setter Property="FontSize" Value="{DynamicResource TitleFontSize}"/>
        <Setter Property="Foreground" Value="{StaticResource TextForeground}"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    <Style TargetType="{x:Type TextBlock}" x:Key="Heading1" BasedOn="{StaticResource BaseStyle}">
        <Setter Property="FontSize" Value="{DynamicResource HeaderFontSize}"/>
        <Setter Property="Foreground" Value="{StaticResource TextForeground}"/>
    </Style>
    <Style TargetType="{x:Type TextBlock}" x:Key="Heading2" BasedOn="{StaticResource BaseStyle}">
        <Setter Property="FontSize" Value="{DynamicResource Header2FontSize}"/>
        <Setter Property="Foreground" Value="{StaticResource TextForeground}"/>
    </Style>
    
    <Style TargetType="{x:Type TextBlock}" x:Key="ContentText" BasedOn="{StaticResource BaseStyle}">
        <Setter Property="FontSize" Value="{DynamicResource SubjectFontSize}"/>
        <Setter Property="Foreground" Value="{StaticResource ContrastForeground}"/>
    </Style>

    <!-- Geometries -->
    <Geometry x:Key="DownArrow">M0,0 L1,0 0.5,1Z</Geometry>
    <Geometry x:Key="UpArrow">M0,1 L1,1 0.5,0Z</Geometry>
    <Geometry x:Key="RightArrow">M0,0 L1,0.5 0,1Z</Geometry>
    <Geometry x:Key="LeftArrow">M0,0.5 L1,1 1,0Z</Geometry>
    <Geometry x:Key="CloseX">M0,0 L1,1 M0,1 L1,0</Geometry>

